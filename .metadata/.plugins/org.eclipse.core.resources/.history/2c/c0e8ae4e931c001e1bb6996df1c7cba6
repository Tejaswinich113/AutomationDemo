package com.javapractice.javademo;

public class VariablesDemo {
	byte age=24;
	short stdnid=219;
	int banknumber=234567;
	char grade='a';
	long collegefee=100000;
	float stdnpercentage=85.3f;
	double stdnattendence=90.87654;
	boolean condition=true;
	
	public static void main(String[] args) {
		VariablesDemo teja=new VariablesDemo();
		System.out.println(teja.age);
		System.out.println(teja.banknumber);
	}
}
